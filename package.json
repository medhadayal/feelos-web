{
  "name": "feelos-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "env:sync": "node scripts/env-sync.mjs",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:status": "docker compose ps",
    "db:logs": "docker compose logs -f db",
    "db:generate": "npm run env:sync && prisma generate",
    "db:migrate": "npm run env:sync && prisma migrate dev --name init",
    "db:seed": "npm run env:sync && prisma db seed",
    "db:setup:nodocker": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:setup": "node scripts/db-setup.mjs",
    "preinstall": "node -e \"try{require('fs').rmSync('node_modules/@prisma',{recursive:true,force:true});require('fs').rmSync('node_modules/prisma',{recursive:true,force:true})}catch(e){}\"",
    "postinstall": "node -e \"try{require('child_process').execSync('npx prisma generate',{stdio:'inherit'})}catch(e){process.exit(0)}\"",
    "predev": "node -e \"try{require('fs').rmSync('.next/dev/lock',{force:true})}catch(e){}\""
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.554.0",
    "mammoth": "^1.11.0",
    "next": "16.0.4",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
